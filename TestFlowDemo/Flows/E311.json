{
  "Model": "E311",
  "TestSteps": [
    {
      "Name": "power_on",
      "Description": "上电",
      "Type": "Normal",
      "Target": "power_supply_1",
      "Command": "set_voltage",
      "Parameters": { "voltage": 12.0 },
      "ExpectedResults": {
        "mode": "equals",
        "key": "status",
        "value": "ok"
      },
      "TimeoutMs": 2000,
      "DependsOn": [],
      "OnSuccess": "mydriver_demo",
      "OnFailure": "power_off"
    },
    {
      "Name": "mydriver_demo",
      "Description": "插件设备示例步骤",
      "Type": "Normal",
      "Target": "my_driver_1",
      "Command": "demo",
      "Parameters": {},
      "ExpectedResults": {
        "mode": "exists",
        "key": "status"
      },
      "TimeoutMs": 1000,
      "DependsOn": [  ],
      "OnSuccess": "ecu_can_check_ref",
      "OnFailure": "power_off"
    },
    {
      "Name": "ecu_can_check_ref",
      "Description": "CAN 报文测试",
      "Type": "SubFlowRef",
      "Ref": "ecu_can_check",
      "DependsOn": [  ],
      "OnSuccess": "ecu_iv_check_ref",
      "OnFailure": "power_off"
    },
    {
      "Name": "ecu_iv_check_ref",
      "Description": "电流电阻联动检查",
      "Type": "SubFlowRef",
      "Ref": "ecu_iv_check",
      "DependsOn": [  ],
      "OnSuccess": "power_off",
      "OnFailure": "power_off"
    },
    {
      "Name": "power_off",
      "Description": "下电",
      "Type": "Normal",
      "Target": "power_supply_1",
      "Command": "set_voltage",
      "Parameters": { "voltage": 0.0 },
      "ExpectedResults": {
        "mode": "equals",
        "key": "status",
        "value": "ok"
      },
      "TimeoutMs": 2000,
      "DependsOn": [  ],
      "OnSuccess": "generate_report",
      "OnFailure": "generate_report"
    },
    {
      "Name": "generate_report",
      "Description": "生成报告",
      "Type": "Normal",
      "Target": "report_generator",
      "Command": "generate",
      "Parameters": { "format": "html" },
      "ExpectedResults": {},
      "TimeoutMs": 2000,
      "DependsOn": [  ],
      "OnSuccess": "end_test",
      "OnFailure": "end_test"
    },
    {
      "Name": "end_test",
      "Description": "测试结束",
      "Type": "Normal",
      "Target": "system",
      "Command": "shutdown",
      "Parameters": {},
      "ExpectedResults": {},
      "TimeoutMs": 1000,
      "DependsOn": [  ],
      "OnSuccess": "",
      "OnFailure": ""
    }
  ]
}
